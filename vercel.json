{
    "version": 2,
    "builds": [
      {
        "src": "src/index.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/(.*)",
        "dest": "src/index.mjs",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/(.*)",
        "dest": "src/index.mjs"
      }
    ]
  }